{
  "name": "stayfit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && cross-env NODE_ENV=production vite build",
    "build:client": "cross-env NODE_ENV=production vite build || echo 'Build failed.'",
    "build:server": "cross-env NODE_ENV=production tsc",
    "dev": "cross-env NODE_ENV=development vite & nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm --no-warnings' server/server.ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"{src,server}/**/*.{js,jsx,ts,tsx}\" --quiet",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=production tsx server/server.ts",
    "test": "cross-env NODE_ENV=test vitest --run",
    "test:coverage": "cross-env NODE_ENV=test vitest --coverage",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.15.0",
    "redux-persist": "^6.0.0",
    "ts-loader": "^9.4.4",
    "tsx": "^3.12.7"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/react": "^14.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.34.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "happy-dom": "^10.11.2",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.18",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  }
}
